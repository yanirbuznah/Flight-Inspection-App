<UserControl x:Class="Flight_Inspection_App.Controls.FeaturesGraphs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Flight_Inspection_App.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="270">
    <Grid Background="Transparent">
        <Canvas Name="feature1Graph" Background="White"
             Width="122" Height="122"
            VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,10,0,0"/>
        <Canvas Name="feature2Graph" Background="White"
             Width="123" Height="123"
            VerticalAlignment="Top" HorizontalAlignment="Left" Margin="137,9,0,0"/>
        <Canvas Name="regressionGraph" Background="White"
             Width="250" Height="250"
            VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,137,0,0"/>
    </Grid>

</UserControl>
